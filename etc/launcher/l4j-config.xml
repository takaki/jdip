<launch4jConfig>
	<headerType>0</headerType>
	<jar>..\..\build\jardist\jdip.jar</jar>
	<outfile>..\..\build\jardist\jdip.exe</outfile>
	
	<!-- icon in .ico format; should have 64, 32, and 16 in one -->
	
	<jre>
