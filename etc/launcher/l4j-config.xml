		<maxHeapSize>192</maxHeapSize>
		<args>Xss1024K</args>
	</jre>
	<versionInfo>
		<fileVersion>1.7.0</fileVersion>
		<txtFileVersion>jDip 1.7.0</txtFileVersion>
		<fileDescription>jDip</fileDescription>
		<copyright>2005</copyright>
		<productVersion>1.7.0</productVersion>
		<txtProductVersion>text</txtProductVersion>
		<productName>jDip</productName>
		<companyName>http://jdip.sourceforge.net/</companyName>
		<internalName>jDip</internalName>
		<originalFilename>jdip.exe</originalFilename>
	</versionInfo>
</launch4jConfig>	
	
	
	
